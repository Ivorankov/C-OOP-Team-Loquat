<Window x:Class="WpfApplication1.LaptopWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:system="clr-namespace:System;assembly=mscorlib"
        Title="Laptop" MinHeight="300" MaxHeight="450" Width="400">
    <Window.Resources>
        <ResourceDictionary Source="../../Styles.xaml" />
    </Window.Resources>
    <StackPanel>
        <TextBlock Text="Laptop Player"></TextBlock>
        <TextBox Text="" x:Name="LaptopInformation"/>
        <Button Content="Checkout" Click="AddToCart_Click"/>
    </StackPanel>
</Window>
