<Window x:Class="WpfApplication1.DesktopWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Desktop" Height="300" Width="400">
    <Window.Resources>
        <ResourceDictionary Source="../../Styles.xaml" />
    </Window.Resources>
    <StackPanel>
        <TextBlock Text="Desktop"></TextBlock>
        <TextBox Text="Desktop Information" x:Name="TextBox"/>
        <Button Content="Checkout" Click="AddToCart_Click"/>
    </StackPanel>
</Window>
