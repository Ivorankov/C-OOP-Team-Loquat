        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="GSMWindow" MinHeight="300" MaxHeight="450" MinWidth="700" MaxWidth="700">
    <Window.Resources>
        <ResourceDictionary Source="../../Styles.xaml" />
    </Window.Resources>
    <StackPanel>
        <TextBlock Text="GSM"></TextBlock>
        <TextBlock Text="GSM Information" x:Name="TextBlock"/>
        <Button Content="Add to cart" Click="AddToCart_Click"/>
    </StackPanel>
</Window>
