
<Window x:Class="WpfApplication1.SideWindows.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RegisterWindow" Height="300" Width="300" FontFamily="Verdana" Background="White">
    <Window.Resources>
        <ResourceDictionary Source="../../Styles.xaml" />
    </Window.Resources>
    <StackPanel>
        <TextBlock Text="Registration"></TextBlock>
        <TextBox x:Name="TextBox" Text="Enter User Name..." />
        <PasswordBox x:Name="TextBox1" PasswordChar="*" />
        <TextBox x:Name="TextBox2" Text="Enter Email..." />
        <Button Content="Register" Click="Button_Click"/>
    </StackPanel>
</Window>
