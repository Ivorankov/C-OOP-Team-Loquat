<Window x:Class="WpfApplication1.UserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="UserWindow" Height="300" Width="300">
    <Window.Resources>
        <ResourceDictionary Source="../../Styles.xaml" />
    </Window.Resources>
    <StackPanel>
        <TextBlock Text="User Login"></TextBlock>
        <TextBox x:Name="TextBox" Text="User Name" />
        <PasswordBox x:Name="TextBox1" PasswordChar="*" />
        <Button Content="Submit" Click="Button_Click"/>
    </StackPanel>
</Window>
