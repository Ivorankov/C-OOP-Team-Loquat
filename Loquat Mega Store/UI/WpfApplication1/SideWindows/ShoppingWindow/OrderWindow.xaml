<Window x:Class="WpfApplication1.SideWindows.ShoppingWindow.OrderWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="OrderWindow" Height="300" Width="300">
    <Window.Resources>
        <ResourceDictionary Source="../../Styles.xaml" />
    </Window.Resources>
    <StackPanel>
        <TextBlock Text="Finalize Order"></TextBlock>
        <TextBox x:Name="TextBox" Text="Address"/>
        <TextBox x:Name="TextBox1" Text="Contact Name"/>
        <Button Content="Checkout" Background="ForestGreen" Click="Checkout_Click"/>
        <Button Content="Cancel" Background="DarkRed" Click="Cancel_Click"/>
    </StackPanel>
</Window>
