<Window x:Class="WpfApplication1.SideWindows.CartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="CartWindow" Height="300" Width="500">
    <Window.Resources>
        <ResourceDictionary Source="../../Styles.xaml" />
    </Window.Resources>
    <StackPanel>
        <TextBlock Text="Your Cart"></TextBlock>
        <TextBox Text="User Cart Information" x:Name="TextBox"/>
        <Button Content="Checkout" Click="Checkout_Click"/>
    </StackPanel>
</Window>
